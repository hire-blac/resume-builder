{
  "name": "y",
  "version": "1.0.0",
  "description": "resume builder web application using node.js for the backend and react.js for the frontend",
  "main": "index.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "server": "nodemon server",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm install && npm run build"
  },
  "keywords": [
    "react",
    "node",
    "resumebuilder",
    "webapp",
    "express"
  ],
  "author": "danihire",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^6.3.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "html-pdf": "^3.0.1"
  }
}
